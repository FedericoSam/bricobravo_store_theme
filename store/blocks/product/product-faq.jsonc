  "rich-text#pdp-faq-ask-more": {
    "props": {
      "text": "Desideri ulteriori informazioni sul prodotto? ",
      "blockClass": "pdp-faq-ask-more"
    }
  },

  "link#pdp-faq-ask-more": {
    "props": {
      "label": "Fai una domanda",
      "blockClass": "pdp-faq-ask-more",
      "href": "#"
    }
