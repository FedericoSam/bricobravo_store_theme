    "children": ["flex-layout.col#faq"],
    "props": { "blockClass": "pdp-faq" }
  },
  "flex-layout.col#faq": {
    "children": [
      "rich-text#pdp-faq-title",
      "flex-layout.row#pdp-faq-disclousure-layout",
