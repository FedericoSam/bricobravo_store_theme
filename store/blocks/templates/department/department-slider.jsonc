{
  "flex-layout.row#department-category": {
    "children": [
      "rich-text#department-category-title",
      "slider-layout#department-category-slider"
    ],
    "props": {
      "blockClass": ["search-category"],
      "preventHorizontalStretch": true
    }
  },
  "rich-text#department-category-title": {
    "props": {
      "blockClass": ["section-title", "shelf-title", "search-category-title"],
      "font": "t-body",
      "text": "Categorias",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "slider-layout#department-category-slider": {
    "children": [
      "flex-layout.row#department-category-container-1",
      "flex-layout.row#department-category-container-2",
      "flex-layout.row#department-category-container-3",
      "flex-layout.row#department-category-container-4",
      "flex-layout.row#department-category-container-5",
      "flex-layout.row#department-category-container-6",
      "flex-layout.row#department-category-container-7",
      "flex-layout.row#department-category-container-8"
    ],
    "props": {
      "blockClass": ["category-slider", "search-category"],
      "infinite": false,
      "itemsPerPage": {
        "desktop": 8,
        "mobile": 3
      },
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "centerMode": "to-the-left"
    }
  },
  "flex-layout.row#department-category-container-1": {
    "children": ["b8-LinkWrapper#department-1"],
    "props": {
      "blockClass": ["category-container"],
      "preventHorizontalStretch": true
    }
  },
  "b8-LinkWrapper#department-1": {
    "children": [
      "flex-layout.col#department-image-container-1",
      "rich-text#department-category-name-1"
    ],
    "props": {
      "href": "/teste"
    }
  },
  "flex-layout.col#department-image-container-1": {
    "children": ["image#department-category-1"],
    "props": {
      "blockClass": ["image-container"]
    }
  },
  "image#department-category-1": {
    "props": {
      "blockClass": ["category"],
      "maxHeight": 48,
      "src": "https://balarotiio.vteximg.com.br/arquivos/pisos.svg"
    }
  },
  "rich-text#department-category-name-1": {
    "props": {
      "blockClass": ["category-name"],
      "text": "Pisos e Revestimentos",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
