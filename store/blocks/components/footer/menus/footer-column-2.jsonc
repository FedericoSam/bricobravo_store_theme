{
  "flex-layout.col#column-2": {
    "title": "Coluna 2",
    "children": [
      "rich-text#footer-col-2",
      "beightone.menu@0.x:menu#footer-col-2"
    ],
    "props": {
      "blockClass": "footer-cols",
      "preventVerticalStretch": true
    }
  },
