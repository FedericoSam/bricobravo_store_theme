  // Mobile
  "disclosure-layout#column-2": {
    "title": "CollapseMenu: Institucional",
    "children": ["disclosure-trigger#2", "disclosure-content#2"],
    "props": {
      "blockClass": "disclosure-layout-container",
      "initialVisibility": "hidden"
    }
  },
  "disclosure-trigger#2": {
    "title": "CollapseMenu: Gatilho do Menu",
    "children": ["flex-layout.row#trigger-container-2"],
    "props": {
      "blockClass": "footer-menu-trigger",
      "as": "div"
    }
  },

  "flex-layout.row#trigger-container-2": {
    "title": "Linha: Conteúdo de Institucional",
    "children": ["rich-text#footer-col-2", "disclosure-state-indicator"],
    "props": {
      "blockClass": ["disclosure-trigger-content"],
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true
    }
  },

  "disclosure-content#2": {
    "title": "CollapseMenu: Conteúdo de Institucional",
    "children": ["beightone.menu@0.x:menu#footer-col-2"],
    "props": {
      "blockClass": ["footer-content"],
      "text": "Institucional"
    }
  }
}
