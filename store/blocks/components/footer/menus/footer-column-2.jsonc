      "orientation": "vertical",
      "blockClass": "footer-menu",
      "items": [
        {
          "id": "menu-item-footer-col-2-1",
          "type": "custom",
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "atendimento",
            "noFollow": false,
            "tagTitle": "Atendimento",
            "text": "Atendimento"
          }
        },
        {
          "id": "menu-item-footer-col-2-2",
          "type": "custom",
          "highlight": false,
          "itemProps": {
